{
  "name": "oslu",
  "description": "On Screen Log Utils",
  "version": "0.20.0",
  "license": "MIT",
  "scripts": {
    "test": "vitest --ui",
    "lint": "pnpm tsc && pnpm eslint && pnpm format",
    "eslint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "format": "prettier --write src/**/*.ts",
    "tsc": "tsc -p tsconfig.prod.json",
    "test:run": "vitest run",
    "build": "pnpm test:run && pnpm lint && pnpm build:no-test",
    "build:no-test": "tsup --dts",
    "npm-publish": "./scripts/check-if-is-sync.sh && pnpm build && npm publish"
  },
  "files": [
    "dist"
  ],
  "repository": "github:lucasols/oslu",
  "author": "Lucas Santos",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@ls-stack/eslint-cfg": "^0.1.4",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-jest": "^29.0.1",
    "prettier": "^3.6.2",
    "tsm": "^2.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "jiti": "^2.5.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@lucasols/utils": "^4.31.0",
    "goober": "^2.1.16"
  }
}
