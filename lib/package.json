{
  "name": "oslu",
  "description": "On Screen Log Utils",
  "version": "0.18.1",
  "license": "MIT",
  "scripts": {
    "test": "vitest --ui",
    "lint": "pnpm tsc && pnpm eslint && pnpm format",
    "eslint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "format": "prettier --write src/**/*.ts",
    "tsc": "tsc -p tsconfig.prod.json",
    "test:run": "vitest run",
    "build": "pnpm test:run && pnpm lint && pnpm build:no-test",
    "build:no-test": "tsup --dts",
    "npm-publish": "./scripts/check-if-is-sync.sh && pnpm build && npm publish"
  },
  "files": [
    "dist"
  ],
  "repository": "github:lucasols/oslu",
  "author": "Lucas Santos",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/ui": "^1.5.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.3.0",
    "prettier": "^3.2.5",
    "tsm": "^2.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "dependencies": {
    "@lucasols/utils": "^4.26.2",
    "goober": "^2.1.14"
  }
}
